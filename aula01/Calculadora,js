const leia = require('readline-sync')

//Variaveis
let celcious, fahrenheit

//Entrada de dados
celsius = leia.questionFloat("Digite a temperatura em Celsius: ")

Processamento:
fahrenheit = (celsius * 1.8) + 32

//saIda de dados
console.log("\nA temperatura em Fahrenheit é: " + fahrenheit)
console.log("\nA temperatura em Fahrenheit é: %f" , fahrenheit)